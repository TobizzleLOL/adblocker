{
    "name": "Tobis Adblocker",
    "version": "1.0",
    "permissions": ["webRequest", "webRequestBlocking", "<all_urls>"],
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "content_scripts": [
        {
            "matches": [
                "*://*.youtube.com/*"
            ],
            "js": [
                "contentScript.js"
            ]
        }
    ],
    "manifest_version": 2
}